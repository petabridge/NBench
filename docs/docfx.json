{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.csproj" ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**",
            "**/*Tests*.csproj",
            "**/*Tests.*.csproj"
          ],
          "src": "../src"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      },
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "web.config",
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
     "sitemap": {
       "baseUrl": "https://yoursite.github.io/"
     },
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Petabridge.Library",
      "_disableContribution": "true",
      "_appLogoPath": "/images/icon.png",
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template"
    ],
    "postProcessors": ["ExtractSearchIndex"],
    "noLangKeyword": false
  }
}